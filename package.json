{
  "name": "jest-date-mock-mongoose-friendly",
  "version": "1.1.0",
  "description": "Mock for the Date. A fork of jest-date-mock but Doesn't break the mongoose",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest --no-cache",
    "prepublish": "yarn build",
    "build": "babel src --out-dir lib --copy-files && npm run test",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandr2110pro/jest-date-mock-mongoose-friendly.git"
  },
  "keywords": [
    "mock",
    "jest",
    "jest-date",
    "Date",
    "datetime",
    "timestamp",
    "mongoose"
  ],
  "author": "alexandr2110pro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexandr2110pro/jest-date-mock-mongoose-friendly/issues"
  },
  "homepage": "https://github.com/alexandr2110pro/jest-date-mock-mongoose-friendly#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-version": "^0.2.2",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1"
  },
  "jest": {
    "collectCoverage": true,
    "setupFiles": [
      "./src/index.js"
    ]
  }
}
